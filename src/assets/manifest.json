{
  "manifest_version": 2,
  "name": "Note",
  "version": "1.0.0",
  "permissions": [ "tabs", "activeTab", "storage"],
  "background": {
    "page": "index.html#/event-page",
    "persistent": false
  },
  "browser_action": {
    "default_title": "Open Popup!",
    "default_popup": "index.html#/popup"
  },
  "icon": "favicon.ico",
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'"
}
